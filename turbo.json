{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "//#clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "exports": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    }
  },
  "globalEnv": ["NEXT_PUBLIC_API_URL"],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event",
    "MONGODB_URI",
    "CLOUDFLARE_TURNSTILE_SECRET_KEY",
    "SERVER_TOKEN",
    "BETTER_AUTH_SECRET",
    "BETTER_AUTH_URL",
    "S3_ACCESS_KEY_ID",
    "S3_SECRET_ACCESS_KEY",
    "S3_REGION",
    "S3_BUCKET"
  ]
}
